page {
  background-color: #dedede;
}
.search-input-wrapper {
  display: flex;
  height: 60rpx;
  margin-top: 20rpx;
}
.search-input-wrapper .search-input {
  flex: 1;
  padding-left: 30rpx;
  margin-left: 30rpx;
  height: 100%;
  background-color: #fff;
  margin-right: 30rpx;
}
.search-input-wrapper .cancel-search {
  width: 110rpx;
  height: 100%;
  padding: 0;
  margin: 0 10rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
}
.search-goods-list {
  margin: 30rpx 0 0 30rpx;
}
.search-goods-list .search-goods-item {
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1px solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  background-color: #fff;
}
